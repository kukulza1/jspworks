   <%
    //배열
    int[] arr = new int[]{10,20,30};
    for(int i=0; i<arr.length;i++){
    	out.print(arr[i]+",");
    }
    out.print("<br>")
    //arrays 클래스
    int arr2[] = new int[]{40,50,60};
    out.print(Arrays.toString(arr2));
   %>
